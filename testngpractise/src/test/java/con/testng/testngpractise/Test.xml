<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" verbose="1">
<test name="ParameterTest">
        <parameter name="username" value="" />
        <parameter name="password" value="" />
        <parameter name="url" value="http://example.com" />
        <classes>
            <class name="con.testng.testngpractise.Parameter" />
        </classes>
    </test>
    
   
    <test name="Smoke Tests">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <classes>
            <class name="ExampleTest"/>
        </classes>
    </test>

    <test name="Regression Tests">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="ExampleTest"/>
        </classes>
    </test>
    
    <test name="SmokeTests">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <classes>
            <class name="con.testng.testngpractise.TestNGGroupExample"/>
        </classes>
    </test>

    <test name="RegressionTests">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="con.testng.testngpractise.TestNGGroupExample"/>
        </classes>
    </test>

    <test name="IntegrationTests">
        <groups>
            <run>
                <include name="integration"/>
            </run>
        </groups>
        <classes>
            <class name="con.testng.testngpractise.TestNGGroupExample"/>
        </classes>
    </test>
    
    <test name="MathTests">
        <groups>
            <run>
                <include name="math"/>
            </run>
        </groups>
        <classes>
            <class name="your.package.name.MainAppTest"/>
        </classes>
    </test>
    
    <test name="Database Tests">
        <groups>
            <run>
                <include name="database"/>
            </run>
        </groups>
        <classes>
            <class name="con.testng.testng.AccountManagerTest"/>
        </classes>
    </test>
    <test name="Export Tests">
        <groups>
            <run>
                <include name="export"/>
            </run>
        </groups>
        <classes>
            <class name="con.testng.testng.AccountManagerTest"/>
        </classes>
    </test>
</suite>
